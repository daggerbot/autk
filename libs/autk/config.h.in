#ifndef AUTK_CONFIG_H_INCLUDED
#define AUTK_CONFIG_H_INCLUDED

#define AUTK_VERSION "@CMAKE_PROJECT_VERSION@"
#define AUTK_VERSION_MAJOR @CMAKE_PROJECT_VERSION_MAJOR@
#define AUTK_VERSION_MINOR @CMAKE_PROJECT_VERSION_MINOR@
#define AUTK_VERSION_PATCH @CMAKE_PROJECT_VERSION_PATCH@

#define AUTK_DRIVER_WIN32 1
#define AUTK_DRIVER_COCOA 2
#define AUTK_DRIVER_X11 3
#define AUTK_DRIVER_WAYLAND 4

#define AUTK_DRIVER AUTK_DRIVER_@AUTK_DRIVER@
#define AUTK_ICONV @AUTK_ICONV@
#define AUTK_MAIN @AUTK_MAIN@
#define AUTK_PIC @AUTK_PIC@
#define AUTK_SHARED @AUTK_SHARED@

#ifndef AUTK_HIDDEN
# if defined(__GNUC__) && !defined(_WIN32)
#  define AUTK_HIDDEN __attribute__((__visibility__("hidden")))
# else
#  define AUTK_HIDDEN
# endif
#endif

#ifndef AUTK_IMPORT
# if AUTK_SHARED && defined(_WIN32)
#  define AUTK_IMPORT __declspec(dllimport)
# else
#  define AUTK_IMPORT
# endif
#endif

#endif // AUTK_CONFIG_H_INCLUDED
